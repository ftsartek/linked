{
  "info": {
    "title": "Litestar API",
    "version": "1.0.0"
  },
  "openapi": "3.1.0",
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/auth/callback": {
      "get": {
        "summary": "Callback",
        "operationId": "AuthCallbackCallback",
        "parameters": [
          {
            "name": "code",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "required": true,
            "deprecated": false,
            "allowEmptyValue": false,
            "allowReserved": false
          },
          {
            "name": "oauth_state",
            "in": "query",
            "schema": {
              "type": "string"
            },
            "required": true,
            "deprecated": false,
            "allowEmptyValue": false,
            "allowReserved": false
          }
        ],
        "responses": {
          "200": {
            "description": "Redirect Response",
            "headers": {
              "location": {
                "schema": {
                  "type": "string"
                },
                "description": "target path for the redirect",
                "required": false,
                "deprecated": false
              }
            }
          },
          "400": {
            "description": "Bad request syntax or unsupported method",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "status_code": {
                      "type": "integer"
                    },
                    "detail": {
                      "type": "string"
                    },
                    "extra": {
                      "additionalProperties": {},
                      "type": [
                        "null",
                        "object",
                        "array"
                      ]
                    }
                  },
                  "type": "object",
                  "required": [
                    "detail",
                    "status_code"
                  ],
                  "description": "Validation Exception",
                  "examples": [
                    {
                      "status_code": 400,
                      "detail": "Bad Request",
                      "extra": {}
                    }
                  ]
                }
              }
            }
          }
        },
        "deprecated": false
      }
    },
    "/auth/link": {
      "get": {
        "summary": "Link",
        "operationId": "AuthLinkLink",
        "responses": {
          "200": {
            "description": "Redirect Response",
            "headers": {
              "location": {
                "schema": {
                  "type": "string"
                },
                "description": "target path for the redirect",
                "required": false,
                "deprecated": false
              }
            }
          }
        },
        "deprecated": false
      }
    },
    "/auth/login": {
      "get": {
        "summary": "Login",
        "operationId": "AuthLoginLogin",
        "responses": {
          "200": {
            "description": "Redirect Response",
            "headers": {
              "location": {
                "schema": {
                  "type": "string"
                },
                "description": "target path for the redirect",
                "required": false,
                "deprecated": false
              }
            }
          }
        },
        "deprecated": false
      }
    },
    "/auth/logout": {
      "post": {
        "summary": "Logout",
        "operationId": "AuthLogoutLogout",
        "responses": {
          "201": {
            "description": "Document created, URL follows",
            "headers": {},
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "boolean"
                  },
                  "type": "object"
                }
              }
            }
          }
        },
        "deprecated": false
      }
    },
    "/auth/me": {
      "get": {
        "summary": "Me",
        "operationId": "AuthMeMe",
        "responses": {
          "200": {
            "description": "Request fulfilled, document follows",
            "headers": {},
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserInfo"
                }
              }
            }
          }
        },
        "deprecated": false
      }
    }
  },
  "components": {
    "schemas": {
      "CharacterInfo": {
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "corporation_id": {
            "oneOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ]
          },
          "alliance_id": {
            "oneOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "type": "object",
        "required": [
          "id",
          "name"
        ],
        "title": "CharacterInfo"
      },
      "UserInfo": {
        "properties": {
          "id": {
            "type": "integer"
          },
          "characters": {
            "items": {
              "$ref": "#/components/schemas/CharacterInfo"
            },
            "type": "array"
          }
        },
        "type": "object",
        "required": [
          "characters",
          "id"
        ],
        "title": "UserInfo"
      }
    }
  }
}